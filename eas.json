{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.incrypt.network"
      },
      "channel": "demo",
      "android": {
        "buildType": "apk",
        "newArchEnabled": false
      },
      "ios": {
        "newArchEnabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.incrypt.network"
      },
      "channel": "demo",
      "android": {
        "buildType": "apk",
        "newArchEnabled": false
      },
      "ios": {
        "newArchEnabled": false
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.incrypt.network"
      },
      "android": {
        "buildType": "app-bundle",
        "newArchEnabled": false
      },
      "ios": {
        "newArchEnabled": false
      }
    }
  },
  "submit": {
    "production": {}
  }
} 