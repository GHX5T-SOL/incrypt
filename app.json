{
  "expo": {
    "name": "Incrypt",
    "slug": "incrypt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "imageResizeMode": "contain"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.incrypt.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.incrypt.app",
      "permissions": [
        "INTERNET",
        "CAMERA",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f79ed6da-1179-4a55-b54f-7659fe9e773c"
      }
    },
    "scheme": "incrypt",
    "owner": "ghxstxbt",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/f79ed6da-1179-4a55-b54f-7659fe9e773c",
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "requestHeaders": {
        "expo-channel-name": "demo"
      }
    }
  }
}
